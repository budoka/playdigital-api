{
  "name": "playdigital-api",
  "version": "1.0.0",
  "description": "Api wrapper de BraveNewCoin",
  "author": "Facundo Vazquez",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "yarn dev",
    "test": "jest --detectOpenHandles",
    "mocha": "mocha --require ts-node/register src/__tests__/*.ts",
    "dev": "nodemon",
    "build": "yarn run build:ts && yarn run build:js && eslint '*/**/*.{js,ts}' --quiet --fix --no-error-on-unmatched-pattern",
    "ts-build-run": "yarn prebuild && tsc && ts-node -r tsconfig-paths/register dist/index.js",
    "ts-check:watch": "yarn run ts-check --watch",
    "build:ts": "tsc --emitDeclarationOnly",
    "build:js": "babel src -d dist --extensions \".ts,.tsx\" --source-maps inline",
    "ts-check": "tsc --noEmit",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "yarn-upgrade-all": "ncu -u && yarn install --check-files && ncu -u",
    "prebuild": "yarn clean",
    "postbuild": "cp package.json dist/package.json && cd dist && yarn --production",
    "clean": "rimraf dist/*"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "class-validator-jsonschema": "^2.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mysql2": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "routing-controllers-openapi": "^2.0.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "typeorm-routing-controllers-extensions": "^0.2.0",
    "typescript": "^3.9.7",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/cors": "^2.8.6",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.157",
    "@types/mysql2": "github:types/mysql2",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.2.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.52",
    "tsconfig-paths": "^3.9.0"
  }
}
